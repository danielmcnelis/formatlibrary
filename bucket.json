{
    "Version": "2012-10-17",
    "Statement": [{
        "Sid": "AllowCloudFrontServicePrincipalReadOnly",
        "Effect": "Allow",
        "Principal": {
            "Service": "cloudfront.amazonaws.com"
        },
        "Action": "s3:GetObject",
        "Resource": "arn:aws:s3:::formatlibrary/*",
        "Condition": {
            "StringEquals": {
                "AWS:SourceArn": "arn:aws:cloudfront::374701620076:distribution/E3SGFS6YHTP5YU"
            }
        }
    },
    {
        "Sid": "AllowIAMPrincipalManage",
        "Effect": "Allow",
        "Principal": { "AWS": "arn:aws:iam::374701620076:user/formatlibrary-s3"},
        "Action": [
            "s3:*"
        ],
        "Resource": "arn:aws:s3:::formatlibrary/*"
    }]
}
